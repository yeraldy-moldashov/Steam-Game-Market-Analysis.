    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import seaborn as sns\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# 1. –ó–ê–ì–†–£–ó–ö–ê –ò –ü–û–î–ì–û–¢–û–í–ö–ê\n",
        "filename = \"df_clean.csv\" # –í–∞—à —Ñ–∞–π–ª\n",
        "\n",
        "try:\n",
        "    df = pd.read_csv(filename)\n",
        "except:\n",
        "    df = pd.DataFrame()\n",
        "\n",
        "if len(df) > 0:\n",
        "    # --- –§–ò–ö–° –¶–ï–ù ---\n",
        "    def fix_price(x):\n",
        "        try:\n",
        "            return float(str(x).replace('$', '').replace(',', '').replace('USD', '').strip())\n",
        "        except:\n",
        "            return 0.0\n",
        "\n",
        "    df['Price_Clean'] = df['Price'].apply(fix_price)\n",
        "\n",
        "    # –û—á–∏—Å—Ç–∫–∞ —Ä–µ–π—Ç–∏–Ω–≥–∞\n",
        "    df['Reviews_Clean'] = df['Reviews_Summary'].astype(str).str.split(',').str[0].str.strip()\n",
        "    df['Is_Free'] = df['Price_Clean'] == 0\n",
        "\n",
        "    # !!! –ì–õ–ê–í–ù–û–ï –ò–°–ü–†–ê–í–õ–ï–ù–ò–ï !!!\n",
        "    # –°–æ–∑–¥–∞–µ–º –æ—Ç–¥–µ–ª—å–Ω—É—é —Ç–∞–±–ª–∏—Ü—É –¢–û–õ–¨–ö–û –¥–ª—è –ø–ª–∞—Ç–Ω—ã—Ö –∏–≥—Ä\n",
        "    # –≠—Ç–æ –∏—Å–ø—Ä–∞–≤–∏—Ç —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É, —É–±—Ä–∞–≤ —Ç—ã—Å—è—á–∏ \"–Ω—É–ª–µ–≤—ã—Ö\" —Ü–µ–Ω\n",
        "    df_paid = df[df['Price_Clean'] > 0].copy()\n",
        "\n",
        "    print(f\"‚úÖ –î–∞–Ω–Ω—ã–µ –∑–∞–≥—Ä—É–∂–µ–Ω—ã. –í—Å–µ–≥–æ –∏–≥—Ä: {len(df)}\")\n",
        "    print(f\"üí∞ –ò–∑ –Ω–∏—Ö –ø–ª–∞—Ç–Ω—ã—Ö –∏–≥—Ä: {len(df_paid)} (–Ω–∞ –Ω–∏—Ö –±—É–¥–µ–º —Å—Ç—Ä–æ–∏—Ç—å –∞–Ω–∞–ª–∏–∑ —Ü–µ–Ω)\")\n",
        "    print(\"-\" * 50)\n",
        "\n",
        "    # ==========================================\n",
        "    # –ê–ù–ê–õ–ò–ó (–¢–ï–ü–ï–†–¨ –ü–†–ê–í–ò–õ–¨–ù–´–ô)\n",
        "    # ==========================================\n",
        "\n",
        "    # Q1: –°–∞–º—ã–µ –¥–æ—Ä–æ–≥–∏–µ (–±–µ–∑ –∏–∑–º–µ–Ω–µ–Ω–∏–π)\n",
        "    print(\"\\n\" + \"=\"*60)\n",
        "    print(\"Q1: –°–∞–º—ã–µ –¥–æ—Ä–æ–≥–∏–µ –∏–≥—Ä—ã\")\n",
        "    print(\"=\"*60)\n",
        "    top_expensive = df.sort_values(by='Price_Clean', ascending=False).head(3)\n",
        "    for _, row in top_expensive.iterrows():\n",
        "        print(f\"  - {row['Title']}: ${row['Price_Clean']} ({row['Category']})\")\n",
        "\n",
        "    # Q2: –î–æ–º–∏–Ω–∏—Ä—É—é—â–∏–µ –∫–∞—Ç–µ–≥–æ—Ä–∏–∏\n",
        "    print(\"\\n\" + \"=\"*60)\n",
        "    print(\"Q2: –î–æ–º–∏–Ω–∏—Ä—É—é—â–∏–µ –∫–∞—Ç–µ–≥–æ—Ä–∏–∏\")\n",
        "    print(\"=\"*60)\n",
        "    print(df['Category'].value_counts().head(3))\n",
        "\n",
        "\n",
        "    # Q3: –õ—É—á—à–∏–µ —Å–∫–∏–¥–∫–∏ (–°—á–∏—Ç–∞–µ–º –ø–æ df_clean, –µ—Å–ª–∏ —Ç–∞–º –µ—Å—Ç—å –∫–æ–ª–æ–Ω–∫–∞ Discount_float)\n",
        "    print(\"\\n\" + \"=\"*60)\n",
        "    print(\"Q3: –ì–¥–µ –ª—É—á—à–∏–µ —Å–∫–∏–¥–∫–∏?\")\n",
        "    print(\"=\"*60)\n",
        "    if 'Discount_float' in df.columns:\n",
        "        # –£–±–∏—Ä–∞–µ–º –Ω—É–ª–µ–≤—ã–µ —Å–∫–∏–¥–∫–∏, —á—Ç–æ–±—ã –Ω–µ –ø–æ—Ä—Ç–∏—Ç—å —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É\n",
        "        df_discounted = df[df['Discount_float'] > 0]\n",
