        "    rating_stats = df_paid[df_paid['Reviews_Clean'].isin(target_ratings)].groupby('Reviews_Clean')['Price_Clean'].mean()\n",
        "\n",
        "    print(\"–°—Ä–µ–¥–Ω—è—è —Ü–µ–Ω–∞ –ü–õ–ê–¢–ù–û–ô –∏–≥—Ä—ã —Å —ç—Ç–∏–º —Ä–µ–π—Ç–∏–Ω–≥–æ–º:\")\n",
        "    print(rating_stats)\n",
        "\n",
        "    try:\n",
        "        price_best = rating_stats.loc['Overwhelmingly Positive']\n",
        "        price_mixed = rating_stats.loc['Mixed']\n",
        "        print(f\"\\n–®–µ–¥–µ–≤—Ä—ã —Å—Ç–æ—è—Ç –≤ —Å—Ä–µ–¥–Ω–µ–º: ${price_best:.2f}\")\n",
        "        print(f\"–°–µ—Ä–µ–¥–Ω—è—á–∫–∏ —Å—Ç–æ—è—Ç –≤ —Å—Ä–µ–¥–Ω–µ–º: ${price_mixed:.2f}\")\n",
        "\n",
        "        if price_mixed > price_best:\n",
