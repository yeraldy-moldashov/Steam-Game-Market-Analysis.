    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "\n",
        "\n",
        "df['Price_float'] = (\n",
        "    df['Price']\n",
        "    .fillna('0')\n",
        "    .replace(['None', 'Free', 'free', ''], '0')\n",
        "    .replace('[\\$,]', '', regex=True)\n",
        "    .astype(float)\n",
        ")\n",
        "\n",
        "df['Original_Price_float'] = (\n",
        "    df['Original_Price']\n",
        "    .fillna('0')\n",
        "    .replace(['None', 'Free', 'free', ''], '0')\n",
        "    .replace('[\\$,]', '', regex=True)\n",
        "    .astype(float)\n",
        ")\n",
        "\n",
        "\n",
        "df_paid = df[df['Price_float'] > 0].copy()\n",
        "\n",
        "\n",
        "df_paid['Discount_float'] = (\n",
        "    (df_paid['Original_Price_float'] - df_paid['Price_float']) /\n",
        "    df_paid['Original_Price_float'] * 100\n",
        ").round(2).fillna(0)\n",
        "\n",
        "\n",
        "df_paid['Price'] = df_paid['Price_float'].apply(lambda x: f\"${x:g}\")\n",
        "df_paid['Original_Price'] = df_paid['Original_Price_float'].apply(lambda x: f\"${x:g}\")\n",
        "df_paid['Discount'] = df_paid['Discount_float'].apply(lambda x: f\"{x:g}%\")\n",
        "\n",
        "\n",
        "df_paid = df_paid[\n",
        "    ['Title', 'Category', 'Price', 'Original_Price', 'Discount',\n",
        "     'Discount_float', 'Reviews_Summary', 'Release_Date']\n",
        "]\n",
        "\n",
        "\n",
        "df_paid\n"
      ],
      "metadata": {
