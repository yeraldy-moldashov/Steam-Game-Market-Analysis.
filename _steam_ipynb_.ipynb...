        "    print(\"\\n\" + \"=\"*60)\n",
        "    print(\"Q4: –î–æ—Ä–æ–≥–∏–µ –∏–≥—Ä—ã –ª—É—á—à–µ? (–¢–æ–ª—å–∫–æ —Å—Ä–µ–¥–∏ –ø–ª–∞—Ç–Ω—ã—Ö)\")\n",
        "    print(\"=\"*60)\n",
        "\n",
        "    target_ratings = ['Overwhelmingly Positive', 'Mixed']\n",
