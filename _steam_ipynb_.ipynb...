      "source": [
        "import requests\n",
        "from bs4 import BeautifulSoup\n",
        "import pandas as pd\n",
        "import time\n",
        "import random\n",
        "\n",
        "def scrape_steam_absolute_final(target_unique=10500):\n",
        "\n",
        "    tags = {\n",
        "        'Action': 19, 'Strategy': 9, 'RPG': 122, 'Indie': 492, 'Adventure': 21,\n",
        "        'Simulation': 599, 'Casual': 597, 'Sports': 701, 'Racing': 699,\n",
        "        'Massively Multiplayer': 128, 'Horror': 1667, 'Sci-Fi': 3942, 'Open World': 1695,\n",
        "        'Puzzle': 1664, 'VR': 21978, 'Anime': 4085, 'Survival': 1662,\n",
        "        'Platformer': 1625, 'Shooter': 1774, 'Visual Novel': 3799,\n",
        "        'Fighting': 1743, 'Card Game': 1666, 'Turn-Based': 1677, 'RTS': 1676,\n",
        "        'Point & Click': 1698, 'Pixel Art': 3964, 'Cyberpunk': 4115, 'Space': 1654,\n",
        "        'Zombies': 1659, 'Military': 4168, 'Historical': 3987, 'Stealth': 1687,\n",
        "        'Roguelike': 1716, 'Co-op': 1685, 'Dating Sim': 9551, 'Management': 1247,\n",
        "        'Building': 1643, 'Funny': 4136, 'Story Rich': 1742, 'Mystery': 1698,\n",
        "        'Fantasy': 1684, 'Magic': 4046, 'Dungeon Crawler': 1720, 'Robots': 3955,\n",
        "        'Sandbox': 3814, 'Physics': 3968, 'Hack and Slash': 1646, 'Classic': 1693\n",
        "    }\n",
        "\n",
        "\n",
        "    items_per_tag = 500\n",
        "\n",
        "    all_games = []\n",
        "    seen_titles = set()\n",
        "\n",
        "    print(f\"üöÄ –ó–ê–ü–£–°–ö –ú–ê–ö–°–ò–ú–ê–õ–¨–ù–û–ì–û –°–ë–û–†–ê!\")\n",
        "    print(f\"–¶–µ–ª—å: –°–æ–±—Ä–∞—Ç—å >10,000 —É–Ω–∏–∫–∞–ª—å–Ω—ã—Ö –∏–≥—Ä, —Å–∫–∞–Ω–∏—Ä—É—è 50 –∫–∞—Ç–µ–≥–æ—Ä–∏–π.\")\n",
        "\n",
        "    headers = {\n",
        "        'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36'\n",
        "    }\n",
        "\n",
        "    base_url_template = \"https://store.steampowered.com/search/?tags={tag_id}&start={start}&count=50&infinite=1\"\n",
        "\n",
        "    for genre_name, tag_id in tags.items():\n",
        "        if len(all_games) > 12500:\n",
        "            print(\"\\nüõë –°–æ–±—Ä–∞–Ω–æ –¥–æ—Å—Ç–∞—Ç–æ—á–Ω–æ –¥–∞–Ω–Ω—ã—Ö —Å –∑–∞–ø–∞—Å–æ–º! –û—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º—Å—è.\")\n",
        "            break\n",
        "\n",
        "        print(f\"\\n--- –°–∫–∞–Ω–∏—Ä—É–µ–º: {genre_name} ---\")\n",
        "        added_count = 0\n",
        "\n",
        "        for start in range(0, items_per_tag + 50, 50):\n",
        "            url = base_url_template.format(tag_id=tag_id, start=start)\n",
        "\n",
        "            try:\n",
        "                r = requests.get(url, headers=headers)\n",
        "                if r.status_code != 200:\n",
        "                    time.sleep(1)\n",
        "                    continue\n",
        "\n",
        "                json_data = r.json()\n",
        "                html_code = json_data.get('results_html', '')\n",
        "\n",
        "                if not html_code: break\n",
        "\n",
        "                soup = BeautifulSoup(html_code, 'html.parser')\n",
        "                rows = soup.find_all('a', class_='search_result_row')\n",
        "\n",
        "                if not rows: break\n",
        "\n",
        "                for row in rows:\n",
        "                    try:\n",
        "                        title = row.find('span', class_='title').text.strip()\n",
        "\n",
        "                        if title in seen_titles:\n",
        "                            continue\n",
        "\n",
        "                        seen_titles.add(title)\n",
        "\n",
        "                        try: release = row.find('div', class_='search_released').text\n",
        "                        except: release = \"\"\n",
        "\n",
        "\n",
        "                        price = None\n",
        "                        orig = None\n",
        "\n",
        "                        discount_block = row.find('div', class_='search_discount_block')\n",
        "                        if discount_block and \"no_discount\" not in discount_block.get('class', []):\n",
        "                            try: orig = row.find('div', class_='discount_original_price').text\n",
        "                            except: orig = None\n",
        "                            try: price = row.find('div', class_='discount_final_price').text\n",
        "                            except: price = None\n",
        "                        else:\n",
        "                            try:\n",
        "                                price_div = row.find('div', class_='search_price')\n",
        "                                if price_div:\n",
        "                                    price_text = price_div.text.strip()\n",
        "                                    if \"Free\" in price_text or \"Play\" in price_text:\n",
        "                                        price = \"Free\"\n",
        "                                        orig = \"Free\"\n",
        "                                    elif price_text == \"\":\n",
        "                                        price = \"Unknown\"\n",
        "                                    else:\n",
        "                                        price = price_text\n",
        "                                        orig = price_text\n",
        "                            except: price = None\n",
        "\n",
        "                        try:\n",
        "                            review_tag = row.find('span', class_='search_review_summary')\n",
        "                            if review_tag:\n",
        "                                review_raw = review_tag.get('data-tooltip-html', '')\n",
        "                                review_clean = review_raw.split('<br>')[0]\n",
        "                            else:\n",
        "                                review_clean = \"No Reviews\"\n",
        "                        except:\n",
        "                            review_clean = \"No Reviews\"\n",
        "\n",
        "                        all_games.append({\n",
        "                            'Title': title,\n",
        "                            'Category': genre_name,\n",
        "                            'Price': price,\n",
        "                            'Original_Price': orig,\n",
        "                            'Reviews_Summary': review_clean,\n",
        "                            'Release_Date': release\n",
        "                        })\n",
        "                        added_count += 1\n",
        "\n",
        "                    except: continue\n",
        "\n",
        "                time.sleep(0.3)\n",
        "\n",
        "            except Exception as e:\n",
        "                time.sleep(1)\n",
        "\n",
        "        print(f\"  + –î–æ–±–∞–≤–ª–µ–Ω–æ {added_count} –Ω–æ–≤—ã—Ö (–í—Å–µ–≥–æ —É–Ω–∏–∫–∞–ª—å–Ω—ã—Ö: {len(all_games)})\")\n",
        "\n",
        "    return all_games\n",
        "\n",
        "\n",
        "data = scrape_steam_absolute_final()\n",
        "\n",
        "\n",
        "if len(data) > 0:\n",
        "    df = pd.DataFrame(data)\n",
        "\n",
        "\n",
        "    df = df.drop_duplicates(subset=['Title'])\n",
        "\n",
        "\n",
        "    df = df.reset_index(drop=True)\n",
        "\n",
        "\n",
        "    df.index = df.index + 1\n",
        "\n",
        "\n",
        "    df.to_csv(\"steam_10k.csv\", index_label='ID')\n",
        "\n",
        "    print(f\"\\n\" + \"=\"*40)\n",
        "    print(f\"–ò–¢–û–ì–û–í–´–ô –†–ê–ó–ú–ï–†: {len(df)} –°–¢–†–û–ö\")\n",
        "    print(\"=\"*40)\n",
        "\n",
        "    if len(df) >= 10000:\n",
        "        print(\"‚úÖ –£–°–ü–ï–•! –ú—ã —Å–æ–±—Ä–∞–ª–∏ –±–æ–ª–µ–µ 10,000 —É–Ω–∏–∫–∞–ª—å–Ω—ã—Ö –∏–≥—Ä.\")\n",
        "    else:\n",
        "        print(f\"‚ö†Ô∏è –°–æ–±—Ä–∞–Ω–æ {len(df)}. –ï—Å–ª–∏ –Ω—É–∂–Ω–æ –µ—â–µ, –ø—Ä–æ—Å—Ç–æ –¥–æ–±–∞–≤—å—Ç–µ —Ç–µ–≥–æ–≤ –≤ —Å–ø–∏—Å–æ–∫ 'tags'.\")\n",
        "\n",
        "    print(df.head())\n",
        "else:\n",
        "    print(\"–û—à–∏–±–∫–∞: –î–∞–Ω–Ω—ã–µ –Ω–µ —Å–æ–±—Ä–∞–Ω—ã.\")"
      ]
    },
