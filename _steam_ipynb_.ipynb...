    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import io\n",
        "\n",
        "\n",
        "df.columns = (\n",
        "    df.columns\n",
        "    .str.strip()\n",
        "    .str.replace('\\xa0', ' ')\n",
        "    .str.replace(' ', '_')\n",
        ")\n",
        "\n",
        "\n",
        "print(df.columns)\n",
        "\n",
        "\n",
        "df_clean = df.copy()\n",
        "\n",
        "df_clean['Price_float'] = (\n",
        "    df_clean['Price']\n",
        "    .fillna('0')\n",
        "    .replace(['None', 'Free', 'free', ''], '0')\n",
        "    .replace('[\\$,]', '', regex=True)\n",
        "    .astype(float)\n",
        ")\n",
        "\n",
        "df_clean['Original_Price_float'] = (\n",
        "    df_clean['Original_Price']\n",
        "    .fillna('0')\n",
        "    .replace(['None', 'Free', 'free', ''], '0')\n",
        "    .replace('[\\$,]', '', regex=True)\n",
        "    .astype(float)\n",
        ")\n",
        "\n",
        "df_clean['Discount_float'] = (\n",
        "    (df_clean['Original_Price_float'] - df_clean['Price_float']) /\n",
        "    df_clean['Original_Price_float'] * 100\n",
        ").round(2).fillna(0)\n",
        "\n",
        "df_clean['Price'] = df_clean['Price_float'].apply(lambda x: f\"${x:g}\")\n",
        "df_clean['Original_Price'] = df_clean['Original_Price_float'].apply(lambda x: f\"${x:g}\")\n",
        "df_clean['Discount'] = df_clean['Discount_float'].apply(lambda x: f\"{x:g}%\")\n",
        "\n",
        "\n",
        "df_clean = df_clean[\n",
        "    ['Title','Category','Price','Original_Price','Discount',\n",
        "     'Discount_float','Reviews_Summary','Release_Date']\n",
        "]\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "df_clean.to_csv(\"df_clean.csv\", index=False)\n",
        "df_clean.to_excel(\"df_clean.xlsx\", index=False)\n",
        "\n",
        "print(\"Clean version saved successfully!\")\n"
      ],
