        "\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "\n",
        "# Create 'Price_float' from the string 'Price' column, removing currency symbols\n",
        "df_paid_filtered = df_paid.copy()\n",
        "df_paid_filtered['Price_float'] = df_paid_filtered['Price'].str.replace('$', '', regex=False).astype(float)\n",
        "\n",
        "# Now filter based on the newly created 'Price_float'\n",
        "df_paid_filtered = df_paid_filtered[df_paid_filtered['Price_float'] > 0].copy()\n",
        "\n",
        "bins = [0, 10, 20, 40, 60, df_paid_filtered['Price_float'].max()]\n",
        "labels = [\"0-10\", \"10-20\", \"20-40\", \"40-60\", \"60+\"]\n",
        "df_paid_filtered['Price_Range'] = pd.cut(df_paid_filtered['Price_float'], bins=bins, labels=labels, include_lowest=True)\n",
        "\n",
        "\n",
        "heatmap_data = df_paid_filtered.pivot_table(\n",
        "    index='Category',\n",
        "    columns='Price_Range',\n",
        "    values='Title',\n",
        "    aggfunc='count',\n",
        "    fill_value=0\n",
        ")\n",
        "\n",
        "\n",
        "plt.figure(figsize=(14, 10))\n",
        "sns.heatmap(\n",
        "    heatmap_data,\n",
        "    annot=True,\n",
        "    fmt='d',\n",
        "    cmap='YlGnBu',\n",
        "    cbar_kws={'label': 'Number of Paid Games'}\n",
        ")\n",
        "\n",
        "plt.title(\"Number of Paid Games by Price Range and Genre\", fontsize=16)\n",
        "plt.xlabel(\"Price Range (S$)\", fontsize=12)\n",
        "plt.ylabel(\"Genre\", fontsize=12)\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ],
